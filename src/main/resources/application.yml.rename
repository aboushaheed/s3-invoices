spring:
  datasource:
    url: "jdbc:mysql://${DATABASE_HOST}/${DATABASE_NAME}"
    username: "${DATABASE_USER}"
    password: "${DATABASE_PASSWORD}"
  jpa:
    hibernate:
      ddl-auto: update

s3:
  main_bucket: "${MAIN_BUCKET}"
  sub_bucket: "${SUB_BUCKET}"
  credential_type: "${CREDENTIAL_TYPE}"
  region: "${REGION}"
billing:
  profile:
    name: "${BILLING_PROFILE_NAME}"